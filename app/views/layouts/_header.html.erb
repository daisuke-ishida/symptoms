<header class="navbar navbar-fixed-top" style="background-color: white">
    <div class="container">
      <div class="navbar-header">
        <button class="navbar-toggle", data-toggle="collapse", data-target="#miniNav">
                <span class="sr-only">ナビゲーション</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span> 
                <span class="icon-bar"></span>
        </button>
        <%= link_to "Let's share the health", "#", id: "logo" %>
      </div>
        <nav>
            <div class="collapse navbar-collapse", id="miniNav">
            <ul class="nav navbar-nav navbar-right">
                <li><%= link_to "Home", root_path %></li>
                <li><%= link_to "Help", "#" %></li>
                <li><%= link_to "お問い合わせ", inquiry_path %></li>
              <% if logged_in? %>
                <li><%= link_to "Log_out", logout_path, method: "delete" %></li>
                <li class="dropkdown">
                    <a herf="#" class="dropdown-toggle" data-toggle="dropdown">
                        Account<b class="caret"></b>
                    </a>
                <ul class="dropdown-menu">
                    <li><%= link_to "Profile", current_user %></li>
                    <li><%= link_to "Settings", "#" %></li>
                    <li><%= link_to "Edit", edit_user_path(current_user) %></li> 
                    <li class="divider"></li>
                    <li>
                        <%= link_to "Log out", logout_path, method: "delete" %>
                    </li>
                </ul>
                </li>
              <% else %>
                <li><%= link_to "Log in", login_path %></li>
              <% end %>
            </ul>
            </div>
        </nav>
    </div>
</header>