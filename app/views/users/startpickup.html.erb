<div class="container-fluid well" style="margin-top:80px">
<div class="container-fluid" style="margin-top: 120px">
  <div class="form-group">
    <div class="col-md-2"></div>
    <div class="col-md-8">
          <%= form_for(@symptom, url:{action:"pickup", controller:"users"}, html:{class:"form-horizontal"}) do |f| %>
          <%= f.label :symptoms, "どんな症状をお探しですか？" %>
          <%= f.collection_check_boxes(:ownerships, Symptom.all,:id,:name) do |s| %>
                <%= s.label { s.check_box + s.text } %>
          <% end %>
    </div>
    <div class="col-md-2"></div>
  </div>
  <div class="form-group">
      <dic class="col-md-4 text-right">
          <%= f.label :name, "症状名" %>
      </div>
      <div class="col-md-4">
          <%= f.text_field :name, class: "form-control" %>
      </div>
      <div class="col-md-4"></div>
  </div>
  <div class="form-group" style="margin-top: 30px">
      <div class="col-md-4"></div>
      <div class="col-md-4">
          <%= f.submit "検索", class: "btn btn-primary" %>
      </div>
     <div class="col-md-4"></div>
<% end %>
</div></br>
<div class="container" style="margin-top: 30px; margin-bottom: 80px">
  <div class="col-md-8"></div>
  <div class="col-md-2">
      <%= link_to "戻る", session_path(current_user.id), method: :get, class: "btn btn-primary" %>
  </div>
  <div class="col-md-2"></div>
</div>
</div>