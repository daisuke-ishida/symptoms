<div class="container-fluid well" style="margin-top: 80px">
<h1>Log in</h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(:session, url: login_path) do |f| %>
        
          <%= f.label :"メールアドレス" %>
          <%= f.email_field :email, class: 'form-control' %>
          
          <%= f.label :"パスワード", style: "padding-top:20px" %>
          <%= f.password_field :password, class: 'form-control' %>
          
      
          <%= f.submit "Log in", class: "btn btn-primary btn-lg", style: "margin-top:40px" %>
        <% end %>
          <p>初めてですか? <%= link_to "登録しましょう!", signup_path %></p>
    </div>
</div>
</form>
</div>